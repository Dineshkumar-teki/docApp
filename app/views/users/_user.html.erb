<tr id="<%= dom_id user %>">
  <td>
    <%= user.first_name %>
  </td>
  <td>
    <%= user.last_name %>
  </td>
  <td>
    <%= user.email %>
  </td>
  <td>
    <%= user.phone %>
  </td>
  <td>
    <%= user.role %>
  </td>
  <td>
      <%= link_to "Show", user%>
  </td>
  <td>
    <%= link_to "Edit", edit_user_path(user) %>
  </td>
  <td>
      <%= link_to "Delete", user, method: :delete %>
  </td>
</tr>

